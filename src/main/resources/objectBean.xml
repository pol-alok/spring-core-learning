<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--	Constructor injection -->

	<bean id="cheat" class="com.alok.CheatSheet"/>
    <bean id="student" class="com.alok.Student">
		<constructor-arg name="sId" value="5"/>
		<constructor-arg name="sName" value="Ram"/>
		<constructor-arg name="cheatSheet" ref="cheat"/>
	</bean>


	<bean id="anotherStudent" class="com.alok.AnotherStudent">
		<constructor-arg name="sId" value="1"/>
		<constructor-arg name="sName" value="Brown"/>
		<constructor-arg name="cheatSheet" ref="cheat"/>
	</bean>


</beans>





