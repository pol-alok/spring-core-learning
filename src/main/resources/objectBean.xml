<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--	setter injection -->
<!--	inner bean-->
    <bean id="student" class="com.alok.Student">
		<property name="sName" value="Ravi"/>
		<property name="sId" value="300"/>
		<property name="cheatSheet">
			<bean class="com.alok.CheatSheet"/>
		</property>
	</bean>
	<bean id="cheat" class="com.alok.CheatSheet"></bean>

<!--	with reference-->
	<bean id="anotherStudent" class="com.alok.AnotherStudent">
		<property name="sName" value="Mohan"/>
		<property name="sId" value="200"/>
		<property name="cheatSheet" ref="cheat"/>
	</bean>
</beans>





